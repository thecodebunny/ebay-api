{
    "name": "thecodebunny/ebay-api",
    "description": "An eBay SDK for PHP. Use the eBay API in your PHP projects.",
    "keywords": ["ebay", "api", "sdk"],
    "homepage": "https://hemangvyas.com/",
    "type": "library",
    "license": "(Apache-2.0 and MIT)",
    "version": "4.0",
    "authors": [
        {
            "name": "Hemang Vyas",
            "email": "info@hemangvyas.com",
            "homepage": "https://hemangvyas.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0",
        "guzzlehttp/psr7": "~1.0",
        "guzzlehttp/promises": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "TheCodeBunny\\eBayApi\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodeBunny\\eBayApi\\Test\\": "test/"
        },
        "classmap": ["build/"]
    }
}
